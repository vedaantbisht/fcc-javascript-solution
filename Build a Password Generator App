//Build a Password Generator App

function generatePassword(len){
  let password = "";
  const pass = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789!@#$%^&*()";

  for(let i = 0;i<len;i++){
    const randomIndex = Math.floor(Math.random()*pass.length);
    password += pass[randomIndex];
  }
  return password;
}

let password = generatePassword(10);
  console.log("Generated password:" + password);

//output:Generated password:UL&V*GNOuc
