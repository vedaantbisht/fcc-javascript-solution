//Implement a DNA Pair Generator
function pairElement(str){
  const pairs ={
    A:"T",
    T:"A",
    C:"G",
    G:"C"
  }

  let result = [];
  for(let i = 0;i<str.length; i++){
    let base = str[i];
    let partner = pairs[base];

    result.push([base, partner]);
  }

  return result;
}
console.log(pairElement("ATCGA"));

// running tests
// tests completed
// console output
[ [ 'A', 'T' ],
  [ 'T', 'A' ],
  [ 'C', 'G' ],
  [ 'G', 'C' ],
  [ 'A', 'T' ] ]
